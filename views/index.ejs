    <div id="view-salaries-sec">
        <div class="container set-pad">
            <div class="row text-center">
                <div>
                    <h1 data-scroll-reveal="enter from the bottom after 0.1s" class="header-line">CAUTA SALARII </h1>
                    <form action="/index#view-salaries-sec" method="post">
                        <div class="form-group">
                            <table width="70%" class="tablesorter" id="resultTable" align="center">
                                <tr>
                                    <td style='padding:2px 2px 2px 2px'>
                                        <input list="companies" name="searchCompanyName" class="form-control" 
                                            placeholder="Oricare Companie" />
                                        <datalist id="companies">
                                            <% for (var index = 0; index < companyNames.length; index++)  { %>
                                                <option value="<%= companyNames[index].companyName %>">
                                            <% } %>
                                        </datalist>
                                    </td>
                                    <td style='padding:2px 2px 2px 2px'>
                                        <input list="jobLocations" name="searchLocation" class="form-control" 
                                            placeholder="Oricare Oras" />
                                        <datalist id="jobLocations">
                                            <% for (var index = 0; index < jobLocations.length; index++)  { %>
                                                <option value="<%= jobLocations[index].location %>">
                                            <% } %>
                                        </datalist>
                                    </td>
                                    <td style='padding:2px 2px 2px 2px'>
                                        <input list="positions" name="searchPosition" class="form-control" 
                                            placeholder="Oricare Pozitie" />
                                        <datalist id="positions">
                                            <% for (var index = 0; index < positions.length; index++)  { %>
                                                <option value="<%= positions[index].position %>">
                                            <% } %>
                                        </datalist>
                                    </td>
                                    <td>
                                        <input type="submit" name="submit_search_query" 
                                            class="btn btn-info"  height="30px" value="CAUTA">
                                    </td>
                                </tr>
                            </table>
                        </div>  
                        <br>
                        <div class="form-group">
                            <table width="80%" style="display:<%= showTable%>;" align="center" id="companyTable">
                            <thead>
                                <tr>
                                    <th style="text-align:center;">Companie ▲ ▼</th>
                                    <th style="text-align:center;">Salariu lunar (NET in lei) ▲ ▼</th>
                                    <th style="text-align:center;">Pozitie ▲ ▼</th>
                                    <th style="text-align:center;">Oras ▲ ▼</th>
                                    <th style="text-align:center;">PFA ▲ ▼</th>
                                    <th style="text-align:center;">Cu Diploma ▲ ▼</th>
                                </tr>
                            </thead>
                            <tbody>
                                <% for (var index = 0; index < searchResults.length; index++)  { %>
                                <tr>
                                        <td style="text-align:center;"><%= searchResults[index].companyName %></td>
                                        <td style="text-align:center;"><%= searchResults[index].salary %></td>
                                        <td style="text-align:center;"><%= searchResults[index].position %></td>
                                        <td style="text-align:center;"><%= searchResults[index].location %></td>
                                        <td style="text-align:center;">
                                            <%
                                                var isPFA = "NU";
                                                if(searchResults[index].isPFA == 1) {
                                                    isPFA = "DA";
                                                } 
                                            %>
                                            <%= isPFA %>
                                        </td>
                                        <td style="text-align:center;">
                                            <%
                                                var hasDiploma = "NU";
                                                if(searchResults[index].hasDiploma == 1) {
                                                    hasDiploma = "DA";
                                                } 
                                            %>
                                            <%= hasDiploma %>
                                        </td>
                                </tr>
                                <% } %>
                            </tbody>
                            </table>
                        </div>
                        <div class="for-group">
                                <p align="center"><%= searchMessage %></p>
                        </div>
                    </form>
                </div>
            </div>
        </div>
    </div>
     
    <div id="add-salary-sec">
        <div class="overlay">
            <div class="container set-pad">
                <div class="row text-center">
                    <div class="col-lg-8 col-lg-offset-2 col-md-8 col-sm-8 col-md-offset-2 col-sm-offset-2">
                        <h1 data-scroll-reveal="enter from the bottom after 0.1s" class="header-line" > ADAUGA SALARII  </h1>
                     </div>
                 </div>
                
                <div class="row set-row-pad"  data-scroll-reveal="enter from the bottom after 0.5s" >
                    <div class="col-lg-8 col-lg-offset-2 col-md-8 col-md-offset-2 col-sm-8 col-sm-offset-2">
                        <form action="/index#add-salary-sec" method="post">
                            <div class="for-group">
                                <p align="center" style="<%= submitMessage.style %>"><%= submitMessage.value %></p>
                            </div>
                            <div class="form-group">
                                <input list="companies" name="companyName" class="form-control" value="<%= formData.companyName.value %>"
                                    placeholder="* Nume Companie" style="<%= formData.companyName.style %>" maxlength="100"/>
                            </div>
                            <div class="form-group">
                                <input list="positions" name="position" class="form-control" value="<%= formData.position.value %>"
                                    placeholder="* Pozitie" style="<%= formData.position.style %>" maxlength="100"/>
                                <datalist id="positions">
                                    <% for (var index = 0; index < positions.length; index++)  { %>
                                        <option value="<%= positions[index].position %>">
                                    <% } %>
                                </datalist>
                            </div>
                            <div class="form-group">
                                <input type="text" name="salary" class="form-control" value="<%= formData.salary.value %>"
                                    placeholder="* Salariu lunar net in LEI" style="<%= formData.salary.style %>" maxlength="100"/> 
                            </div>
                             <div class="form-group">
                                <input list="jobLocations" name="jobLocation" class="form-control" value="<%= formData.jobLocation.value %>"
                                    placeholder="* Oras" style="<%= formData.jobLocation.style %>" maxlength="100"/>
                            </div>
                            <div class="form-group">
                                <input type="text" name="yearsOfExperience" class="form-control" value="<%= formData.experience.value %>" 
                                    placeholder="* Ani Experienta" style="<%= formData.experience.style %>" maxlength="100"/>
                            </div>
                            <div class="form-group">
                                <table width ="30%">
                                    <tr align="center">
                                    <td align="center">
                                        <input type="checkbox" name="isPFA"> PFA 
                                    </td>
                                    <td align-"center">
                                        <input type="checkbox" name="hasDiploma"> Absolvent Facultate 
                                    </td>
                                    </tr>
                                </table>
                            </div>
                            
                            <div class="form-group">
                                <input type="submit" name="submit_salary" 
                                    class="btn btn-info btn-block btn-lg" value="SALVEAZA SALARIUL">
                            </div>
                        </form>
                    </div>      
                </div>
            </div>
        </div> 
    </div>
